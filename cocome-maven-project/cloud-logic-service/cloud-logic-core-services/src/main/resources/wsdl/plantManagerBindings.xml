<!DOCTYPE bindings>
<bindings version="2.1" xmlns="http://java.sun.com/xml/ns/jaxb"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <bindings if-exists="true" schemaLocation="IPlantManager_schema1.xsd">
        <bindings node="//xsd:complexType[@name='EnterpriseTO']">
            <class ref="org.cocome.tradingsystem.inventory.application.store.EnterpriseTO"/>
        </bindings>
    </bindings>
    <bindings if-exists="true" schemaLocation="IPlantManager_schema2.xsd">
        <bindings node="//xsd:complexType[@name='PlantOperationOrderTO']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.recipe.PlantOperationOrderTO"/>
        </bindings>
        <bindings node="//xsd:complexType[@name='PlantOperationOrderEntryTO']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.recipe.PlantOperationOrderEntryTO"/>
        </bindings>
        <bindings node="//xsd:complexType[@name='PlantOperationParameterValueTO']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.recipe.PlantOperationParameterValueTO"/>
        </bindings>
    </bindings>
    <bindings if-exists="true" schemaLocation="IPlantManager_schema3.xsd">
        <bindings node="//xsd:complexType[@name='ProductionUnitTO']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.productionunit.ProductionUnitTO"/>
        </bindings>
        <bindings node="//xsd:complexType[@name='ProductionUnitClassTO']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.productionunit.ProductionUnitClassTO"/>
        </bindings>
        <bindings node="//xsd:complexType[@name='ProductionUnitOperationTO']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.productionunit.ProductionUnitOperationTO"/>
        </bindings>
    </bindings>
    <bindings if-exists="true" schemaLocation="IPlantManager_schema5.xsd">
        <bindings node="//xsd:complexType[@name='PlantTO']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.PlantTO"/>
        </bindings>
    </bindings>
    <bindings if-exists="true" schemaLocation="IPlantManager_schema7.xsd">
        <bindings node="//xsd:complexType[@name='ExpressionInfo']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.expression.ExpressionInfo"/>
        </bindings>
        <bindings node="//xsd:complexType[@name='ConditionalExpressionInfo']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.expression.ConditionalExpressionInfo"/>
        </bindings>
        <bindings node="//xsd:complexType[@name='PUOperationInfo']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.expression.PUOperationInfo"/>
        </bindings>
        <bindings node="//xsd:complexType[@name='MarkupInfo']">
            <class ref="org.cocome.tradingsystem.inventory.application.plant.expression.MarkupInfo"/>
        </bindings>
    </bindings>
</bindings>
